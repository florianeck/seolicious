<% if Seolicious.google_analytics_id.present? %>
  <% if Seolicious.google_tracking_mode == 'ga' %>
  	<script>
  	  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  	  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  	  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  	  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

  	  ga('create', '<%= SocialMediaAndSeo.google_analytics_id %>', 'auto');
  	  ga('send', 'pageview');

  	</script>
  <% else %> 
    <script async src="https://www.googletagmanager.com/gtag/js?id=<%= Seolicious.google_analytics_id %>"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', '<%= Seolicious.google_analytics_id %>');
    </script>
  <% end %>
<% end %>